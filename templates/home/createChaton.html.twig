{% extends 'base.html.twig' %}

{% block title %}Coucou les chatons{% endblock %}

{% block body %}
<section>
        {{ form_start(form) }}
        {{ form_row(form._token) }}
               
                <form class="text-align">
                <div class="form-group">
                        {{ form_row(form.name) }}
                </div>
                <div class="form-group">
                <label for="race">{{ form_row(form.race) }}</label>
                
                </div>
                        <button  type"submit">Valider</button>
                        
                </form>                
                {{ form_rest(form) }}
        {{ form_end(form) }}
                
</section>
{% endblock %}

